@model IEnumerable<ITI.Models.TraineeModel>
<div class="content">
    <div class="col-xs-12">
        <div class="row">
            <div class="row text-right">
                <div class="col-lg-11">

                </div>
                <div class="col-lg-1">
                    <a href="/admin/TraineeResult/TraineeCreate" class="btn btn-primary btn-lg">Create</a>
                </div>
            </div>
            <div class="box">
                <div class="box-body">

                    <div class="row">
                        <table class="table table-bordered">
                            <tr>
                                <th>Trade</th>
                                <th>Average Inspection</th> 
                                <th>Edit</th>
                                <th>Delect</th>
                            </tr>

                            @foreach (var inspection in Model)
                            {
                                <tr>
                                    <td>@inspection.Trade</td>
                                    <td>@inspection.AverageInspection</td>
                                    <td><a href="/admin/TraineeResult/TraineeCreate?id=@inspection.ID" class="update btn btn-warning btn-sm"><span class="glyphicon glyphicon-pencil"></span></a></td>
                                    <td><a href="/admin/TraineeResult/Tdelete?id=@inspection.ID" class="update btn btn-warning btn-sm"><span class="glyphicon glyphicon-trash"></span></a></td>
                                </tr>
                            }
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



