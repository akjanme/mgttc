@model IEnumerable<ITI.Models.ITIResultModel>
<div class="content">
    <div class="col-xs-12">
        <div class="row">
            <div class="row text-right">
                <div class="col-lg-11">

                </div>
                <div class="col-lg-1">
                    <a href="/admin/ITIResult/create" class="btn btn-block btn-primary btn-lg">Create</a>
                </div>
            </div>
            <div class="box">
                <div class="box-body">

                    <div class="row">
                        <table class="table table-bordered">
                            <tr>
                                <th>Trade</th>
                                <th>Sessioin</th>
                                <th>Certification</th>
                                <th></th>
                                <th></th>
                            </tr>

                            @foreach (var res in Model)
                            {
                                <tr>
                                    <td>@res.Trade</td>
                                    <td>@res.Session</td>
                                    <td>@res.CertificateIssued</td>
                                    <td><a href="/admin/ITIResult/create?id=@res.ID" class="update btn btn-warning btn-sm"><span class="glyphicon glyphicon-pencil"></span></a></td>
                                    <td><a href="/admin/ITIResult/delete?id=@res.ID" class="update btn btn-warning btn-sm"><span class="glyphicon glyphicon-trash"></span></a></td>

                                </tr>
                            }
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
