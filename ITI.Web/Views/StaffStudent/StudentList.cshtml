@model IEnumerable<ITI.Models.StudentModel>
<div class="content">
    <div class="col-xs-12">
        <div class="row">
            <div class="row text-right">
            </div>
            <div class="box">
                <div class="box-body">

                    <div class="row">
                        <table class="table table-bordered">
                            <tr>
                                <th>Staff Name</th>
                                <th>Father Name</th>
                                <th>Date of Birth</th>
                            </tr>

                            @foreach (var student in Model)
                            {
                                <tr>
                                    <td>@student.Sname</td>
                                    <td>@student.FatherName</td>
                                    <td>@student.Dob</td>
                                </tr>
                            }
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



